  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch llmg GraphQL",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/lingticio/llmg-graphql/main.go",
      "args": [
        "-e",
        "${workspaceFolder}/cmd/lingticio/llmg-graphql/.env",
      ]
    },
    {
      "name": "Launch llmg gRPC",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/lingticio/llmg-grpc/main.go",
      "args": [
        "-e",
        "${workspaceFolder}/cmd/lingticio/llmg-grpc/.env",
      ]
    }
  ]
}
